{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DB_DATABASE": "bankdata",
        "DB_USERNAME": "postgres",
        "DB_PASSWORD": "1234",
        "DB_PORT": "5432",
        "DB_DIALECT": "postgres",
        "DB_HOST": "localhost",
        "PORT": "3000"
    }
}